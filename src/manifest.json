{
    "manifest_version": 2,
    "name": "Daily Report Writer",
    "version": "1.0",
    "description": "a extention for writing daily report",
    "icons": {
        "16": "ruby.png",
        "48": "ruby.png",
        "128": "ruby.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "ruby.png"
        },
        "default_title": "Daily Report Writer",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "js": ["jquery-3.5.0.min.js", "content_script.js"],
            "matches": ["http://*/*", "https://*/*"]
        }
    ]
}